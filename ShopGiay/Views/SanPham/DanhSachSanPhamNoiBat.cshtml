@model dynamic
<div class="wrap">
    <div class="content-bottom">
        @foreach (var item in Model)
        {
        <div class="box1">
            <div class="col_1_of_3 span_1_of_3">
                <a href="single.html">
                    <div class="view view-fifth">
                        <div class="top_box">
                            <h3 class="m_1">@item.TenSP</h3>
                            <p class="m_2">@item.NHANHIEU.TenNhanHieu</p>
                            <div class="grid_img">
                                <div class="css3"><img src="~/HinhAnh/HinhAnhSP/@item.Anh" style="width:200px;height:173px;" alt="" /></div>
                                <div class="mask">
                                    <div class="info">Xem ngay</div>
                                </div>
                            </div>
                            <div class="price">@item.DonGia</div>
                        </div>
                    </div>
                    <ul class="list">
                        <li>
                            <img src="~/Content/Client/images/plus.png" alt="" />
                            <ul class="icon1 sub-icon1 profile_img">
                                <li>
                                    <a class="active-icon c1" href="#">Thêm vào giỏ</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    <div class="clear"></div>
                </a>
            </div>
        </div>
           
        }
        <div class="clear"></div>
    </div>
</div>


